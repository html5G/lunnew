/* pages/list/list.wxss */
.list {
  padding: 0;
  margin: 0;
}
.list-image {
  display: flex;
  width: 98%;
  padding: 8rpx;
}
.list-image image {
  display: inline-block;
  width: 100%;
  height: 520rpx;
  border-radius: 25rpx;
}
.list-money {
  background-color: rgb(160, 160, 158);
  font-size: 25rpx;
}
.list-describe {
  background-color: rgb(160, 160, 158);
  font-size: 25rpx;
}
.list-button_A {
  background-color: lightsalmon;
  display: flex;
  position: fixed;
  bottom: 0;
  width: 100%;
  border-radius: 25rpx;
}